<script>

  var alternator = 1;

  setInterval(function(){
    if(alternator % 2 == 0){
      $.ajax({
        url: "/a/pages/idea-frag-even",
        success: function(result){
          $("#cl-callout-ideas-div").find(":first-child")[0].remove();
          $("#cl-callout-ideas-div").append(result).hide().fadeIn(999);
        }
      })    
    } else {
      $.ajax({
        url: "/a/pages/idea-frag-odd",
        success: function(result){
          $("#cl-callout-ideas-div").find(":first-child")[0].remove();
          $("#cl-callout-ideas-div").append(result).hide().fadeIn(999);
        }
      })      
    }

    alternator += 1;
  }, 10000);


</script>
