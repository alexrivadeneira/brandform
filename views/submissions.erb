      #if($api.getCurrentMember())

      <img src="$api.getCurrentMember().getAvatar()" alt="$api.getCurrentMember().getName()"/>

      <h2>Welcome, $api.getCurrentMember().getName()</h2>
      #else
      <h2>Your Role</h2>
      #end

      <p class="text-center"><%= selection.welcomeareatext %></p>
      
      <hr class="dark"/>

    </div>
  </section>
  <!-- END general text area -->
<% end %>





<% if selection.activecampaigns %>


  <!-- START active campaigns -->
  #set($campaignCount = 0)
  #foreach($campaign in $api.getActiveCampaigns())
    #set($campaignCount = $campaignCount +1)
  #end

  #if($campaignCount > 0)
  <section class="cl-section cl-campaigns">

      <div class="row cl-row">
        <h2>Active Campaigns</h2>

          <p class="text-center">Browse ongoing initiatives</p>
          <hr class="dark"/>


          #foreach($campaign in $api.getActiveCampaigns())
          <a href="$campaign.getUrl()">
            <div class="col-md-4">
              <div

              #if($campaign.getLogo())
                style="background: url('$campaign.getLogo()') !important; background-size: cover !important;"
              #end

               class="cl-campaign-logo-box landing-pg-bg-accent-1"
          >
                <span class="cl-campaign-title">$textHelper.abbreviate($campaign.getName(), 100)</span>
              </div><!--cl-campaign-logo-box-->
              <article class="cl-campaign-box">
                <p>$textHelper.abbreviate($apiHelper.removeHtml($campaign.getDescription()), 360)</p>
                <span class="cl-campaign-idea-number">
                  <span class="brand-color is-icon is-icon-light-bulb" aria-hidden="true"></span>
                  <span class="sr-only">Number of Ideas:</span> $campaign.getIdeaCount()
                </span><!--cl-campaign-idea-number-->
                #if($campaign.getEndDate())
                  <span class="cl-campaign-end-date">
                    <span class="glyphicon glyphicon-time" aria-hidden="true"> </span>
                    <span class="sr-only">End Date:</span>
                    <time datetime="$apiHelper.formatISO8601Time($campaign.getEndDate())"></time>
                    $apiHelper.formatDate($campaign.getEndDate(), "MMM dd, YYYY")
                  </span><!--cl-campaign-end-date-->
                #end
                </article>        
              </div><!--col-md-4-->
            </a>
        #end
      </div><!-- end row-->

    </section>
    #end
  <!-- END active campaigns -->

<% end %>



<% if selection.horizontalcampaigns %> 

