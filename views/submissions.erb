            <h1><%= selection.heroslide1headerline %></h1>
            <p><%= selection.heroslide1textparagraph %></p>
    

          <% if selection.heroslide1button1name != "" %>
            <a href="<%= selection.heroslide1button1url %>"><button type="submit" class="btn btn-info landing-pg-bg-highlight landing-pg-bc-highlight"><%= selection.heroslide1button1name %></button></a>
